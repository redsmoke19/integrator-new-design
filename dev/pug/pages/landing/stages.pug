section.landing-stages
  .container
    .landing-stages__content
      h2.landing-stages__title 6 основных этапов разработки личного кабинета
    .landing-stages__tabs(data-tabs='parent', data-delay='150')
      ul.landing-stages__controls(data-tabs='controls')
        li.landing-stages__control
          button.landing-stages__control-button(type='button', data-tabs='control')
            +icon("icon-landing-01", "landing-stages__number", "38", "30")
            span.landing-stages__control-head Сбор сведений
          .landing-stages__images-tabs
            img.landing-stages__img(src="static/images/content/landing/stages-1.png" width="459" height="396" alt="Картинка этапа")
        li.landing-stages__control
          button.landing-stages__control-button(type='button', data-tabs='control')
            +icon("icon-landing-02", "landing-stages__number", "38", "30")
            span.landing-stages__control-head Разработка прототипов
          .landing-stages__images-tabs
            img.landing-stages__img(src="static/images/content/landing/stages-2.png" width="459" height="396" alt="Картинка этапа")
        li.landing-stages__control
